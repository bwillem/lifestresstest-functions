    "postinstall": "node node_modules/puppeteer/install.js"
  },
  "devDependencies": {
    "@google-cloud/functions-framework": "^3.1.3",
    "@types/node": "^14.11.2",
    "gts": "^3.1.1",
    "typescript": "~4.7.0"
  },
  "dependencies": {
    "@google-cloud/storage": "^6.9.0",
    "puppeteer": "^19.5.2"
  }
